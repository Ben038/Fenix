
  <div class="sidebar">
    Sidebar
  </div>

<div class="container-dashboard">

  <div class="container-top">

    <div class="container-left">
      <h1>KPI</h1>
      <div class="card-kpi">
        <h1>Commissions Ratio</h1>
        <h2><%= @commission_ratio %></h2>
      </div>
      <div class="card-kpi">
        <h1>Claims Ratio</h1>
        <h2><%= @claim_ratio %></h2>
      </div>
      <div class="card-kpi">
        <h1>Balance Ratio</h1>
        <h2><%= @balance_ratio%></h2>
      </div>
    </div>

    <div class="container-right">

      <div class="card-table">
        <table>
          <tbody>
            <tr>
              <td>Table</td>
              <td>card</td>
              <td>info</td>
              <td>info</td>
            </tr>
            <tr>
              <td><%= @broker_comm_ratio %></td>
              <td><%= @broker_comm_ratio %></td>
              <td><%= @premium %></td>
              <td><%= @claim_paid %></td>
              <td><%= @claim_paid_and_reserves_change %></td>
              <td><%= @premium_and_reserves_change %></td>
              <td><%= @reinsurance_comm %></td>
              <td><%= @broker_comm %></td>
              <td><%= @profit_sharing %></td>
              <td><%= @taxes %></td>
              <td><%= @interests %></td>
              <td><%= @balance %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="container-chart">
        <% 2.times do %>
          <div class="card-chart">
        <div class="card-chart-img">
        </div>
        <div class="card-chart-text">
        Chart card
        </div>
          </div>
        <% end %>
      </div>

    </div>


  </div>

  <div class="container-bottom">
    <h5>News</h5>
    <div class="card-news">
      News card
    </div>
  </div>
</div>

<p> <%= link_to "UPLOAD DATA", new_file_upload_path, class: "btn-yellow" %> </p>
<p> <%= link_to "DIVE INTO...", "#", class: "btn-yellow" %> </p>


