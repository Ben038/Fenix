
<!-- <div class="container-all"> -->

  <div class="container-dashboard">

    <div class="container-top">


    <div class="container-left">
      <div class="card-kpi">
        <p class="kpi-title">Commissions Ratio</p>
        <h3 class="pki-amount"><%= @commission_ratio %></h3>
        <canvas id="kpiCommissions"></canvas>
<!--    <h3 class="pki-amount"> -10 </h3>
        <h3 class="pki-amount"> -15 </h3>
        <h3 class="pki-amount"> -5 </h3> -->
      </div>
      <div class="card-kpi">
        <p class="kpi-title">Claims Ratio</p>
        <h3 class="pki-amount"><%= @claim_ratio %></h3>
      </div>
      <div class="card-kpi">
        <p class="kpi-title">Balance Ratio</p>
        <h3 class="pki-amount"><%= @balance_ratio%></h3>
      </div>
      <div class="card-kpi">
        <p class="kpi-title">Reinsurance Comm Ratio</p>
        <h3 class="pki-amount"><%= @reins_comm_ratio%></h3>
      </div>
      <div class="card-kpi">
        <p class="kpi-title">Broker Comm Ratio</p>
        <h3 class="pki-amount"><%= @broker_comm_ratio%></h3>
      </div>
    </div>

      <div class="container-right">
        <div class="card-table">
          <table>
            <tbody>
              <tr>
                <th scope="col" >  Accounting Item</th>
                <% @data_process.each do |k, v| %>

                <th> <%= k %> </th>
                <% end %>
              </tr>

                <% AccountingDatum.attributes_keys.each do |key| %>

              <tr>
                <th scope="row"> <%= key.to_s %></th>
                 <% @data_process.each do |k, v| %>

                     <td> <%= v[key] %> </th>
                <% end %>
              </tr>

              <% end  %>

            </tbody>
          </table>
        </div>

        <div class="container-chart">
          <div class="card-chart">
            <div class="card-chart-img">
            </div>
            <div class="card-chart-text">
              Premium / Year
              <canvas id="premiumChart"  data-years="<%= @data_process.map { |k, v| k  }.to_json %>" data-process-json="<%= @data_process.map{ |k, v| v[:premium] }.to_json %>">    </canvas>
            </div>
          </div>
          <div class="card-chart">
            <div class="card-chart-img">
            </div>
            <div class="card-chart-text">
              Balance / Year
              <canvas id="balanceChart"></canvas>
            </div>
          </div>
        </div>

      </div>

    </div>

    <div class="container-bottom">
      <div class="card-news">
        <h3>Latest News</h3>
        <p>Prudential Plans to Boost Life Insurance Prices for Vapers</p
      </div>
    </div>

  </div>
