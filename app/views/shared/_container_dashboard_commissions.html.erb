
<div class="container-dashboard" id="dashboard-area">
  <div class="container-top">
    <div class="container-left">
      <div class="card-survey">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
          <div class="card-chart-text">
          Satisfaction Survey
          </div>
          <div class="rating d-flex justify-content-between">
            <h3>Australia</h3>
            <div class="score">
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star"></span>
              <span class="fa fa-star"></span>
            </div>
          </div>
          <div class="rating d-flex justify-content-between">
            <h3>Italy</h3>
            <div class="score">
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star"></span>
            </div>
          </div>
          <div class="rating d-flex justify-content-between">
            <h3>Pakistan</h3>
            <div class="score">
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
            </div>
          </div>
      </div>
    </div>
    <div class="container-right  d-flex">
      <div class="card-chart">
        <div class="card-chart-text">
          Reinsurance Commissions
          <canvas id="reinsComm" width="400" height="400" data-countries="<%= @data_process.map { |k, v| k  }.to_json %>" data-process-json="<%= @data_process.map{ |k, v| v[:reins_comm_ratio] * -1 }.to_json %>"></canvas>
        </div>
      </div>
      <div class="card-chart">
        <div class="card-chart-text">
          Broker Commissions
          <canvas id="brokerComm" width="400" height="400" data-countries="<%= @data_process.map { |k, v| k  }.to_json %>" data-process-json="<%= @data_process.map{ |k, v| v[:broker_comm_ratio] * -1  }.to_json %>"></canvas>
        </div>
      </div>
    </div>
  </div>      <!-- Create an element where the map will take place -->
</div>

